Sign Up Form

<%= form_for @user, url: sign_up_path do |f| %>
  <%= f.label :email %>
  <%= f.email_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.label :password_confirmation%>
  <%= f.password_field :password_confirmation%>

  <%= f.submit "Sign Up" %>

<% end %>
